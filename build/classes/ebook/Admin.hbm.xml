<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="ebook">

    <class name="Admin" table="Admin">
        <id name="id" column="adminID">
            <generator class="native"/>
        </id>
        <property name="name" column="adminName"/>
        <property name="password" column="adminPassword"/>        
        <property name="phone" column="adminPhone"/>
        <property name="email" column="adminEmail"/>        
    </class>  
 
</hibernate-mapping>
